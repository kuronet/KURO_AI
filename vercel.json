{
    "name": "kuro-ai-project",
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/chat.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ]
  }
  